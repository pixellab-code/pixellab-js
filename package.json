{
  "name": "@pixellab-code/pixellab",
  "version": "1.0.0",
  "description": "JavaScript/TypeScript client for Pixel Lab API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixellab-code/pixellab-js.git"
  },
  "homepage": "https://github.com/pixellab-code/pixellab-js#readme",
  "bugs": {
    "url": "https://github.com/pixellab-code/pixellab-js/issues"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["pixellab", "api", "image", "generation", "ai", "typescript"],
  "author": "Pixel Lab",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.22.4",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
